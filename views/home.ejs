<body>
  <div class="container-fluid h1r">
    <h1>Fill Your Details</h1>
  </div>
  <div class="container text-left chill">
    <form class="form-signin" action="/" method="post">
      <div class="form-group">
        <label for="name">Your Full Name</label>
        <input type="text" class="form-control" name="name" value="" placeholder="Your Full name" id="name">
      </div>
      <div class="form-group">
        <label for="section">Section</label>
        <input type="text" class="form-control" name="section" value="" placeholder="Your Section" id="section">
      </div>
      <div class="form-group">
        <label for="rollnum">Your Roll number</label>
        <input type="text" class="form-control" name="rollnum" value="" placeholder="Your Roll Number" id="rollnum">
      </div>
      <button type="submit" class="btn btn-primary" name="button">Next</button>
    </form>
    <br>
  </div>
  <footer class="footer f1">
    <div class="container">
      <span class="">Made in ‚ù§ by Nishant Raj.</span>
    </div>
